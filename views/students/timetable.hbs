<h2>Thời khoá biểu</h2>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <div class="table-responsive">
        <table id="mytable" class="table table-bordred table-striped">
          <thead>
            <th>Mã học phần</th>
            <th>Tên học phần</th>
            <th>Phòng học</th>
            <th>Lịch học</th>
            <th>Tuần bắt đầu</th>
          </thead>
          <tbody>
            {{#each timetable}}
            <tr>
              <td>{{MA_HP}}</td>
              <td>{{TEN_HP}}</td>
              <td>{{PHONG_HOC}}</td>
              <td>Thứ {{LICH_HOC}}: ({{TIET_BAT_DAU}} - {{TIET_KET_THUC}})</td>
              <td>{{NGAY_BAT_DAU}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>